"time" :import
"random" :import

chrono function toChrono -> float fun
	start :time.timeMicros =
	:toChrono
	:time.timeMicros start - 1000000.0 / return
nuf

test -> none fun
	"test\n" :print
	100 1500 :random.randRange :time.sleep
nuf

main -> none fun
	:random.rseed
	test :chrono f"Time to run test: {}s \n" :print
nuf