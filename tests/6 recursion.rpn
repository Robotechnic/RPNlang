factorial int n -> int fun
	n 0 == if
		1 return
	fi
	n 1 - :factorial n * return
nuf

fibonacci int n -> int fun
	n 0 == if
		1 return
	fi
	n 1 == if
		1 return
	fi
	n 1 - :fibonacci n 2 - :fibonacci + return
nuf

main -> none fun
	"Factorial\n" :print 
	4 :factorial f"{}\n" :print
	"Fibonacci\n" :print
	10 :fibonacci f"{}\n" :print
	"Done\n" :print
nuf